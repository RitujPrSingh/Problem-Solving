    "n = int(input().strip())\n",
    "a = list(map(int, input().rstrip().split()))\n",
    "lis=[]\n",
    "for i in range(len(a)):\n",
    "    for j in range(len(a)):\n",
    "        if i != j and a[i] == a[j]:\n",
    "            z=abs(i-j)\n",
    "            lis.append(z)\n",
    "\n",
    "if lis!=[]:          \n",
    "    print(min(lis)) \n",
    "    \n",
    "else:\n",
    "    print('-1')"
