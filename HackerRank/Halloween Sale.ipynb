   "source": [
    "first_multiple_input = input().rstrip().split()\n",
    "p = int(first_multiple_input[0])\n",
    "d = int(first_multiple_input[1])\n",
    "m = int(first_multiple_input[2])\n",
    "s = int(first_multiple_input[3])\n",
    "\n",
    "count = 0\n",
    "while(s>=p):\n",
    "    count = count+1\n",
    "    s=s-p\n",
    "    p =max(p-d,m)\n",
    "    \n",
    "print(count)"
